{
    "name": "linecompletor",
    "displayName": "LineCompletor",
    "description": "Completes a line of code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.completeLine"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.completeLine",
                "title": "Complete Line"
            }
        ],
        "keybindings": [
            {
                "command": "extension.completeLine",
                "key": "ctrl+shift+enter",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}